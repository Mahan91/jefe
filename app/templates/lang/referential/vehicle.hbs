<div class="m-subheader ">
  <div class="d-flex align-items-center">
    <div class="mr-auto">
      <h1 class="m-subheader__title m-subheader__title--separator">
        Vehicle sheet
      </h1>
      <ul class="m-subheader__breadcrumbs m-nav m-nav--inline">
        <li class="m-nav__item m-nav__item--home">
          {{#link-to 'lang' class="m-nav__link m-nav__link--icon"}}
            <i class="m-nav__link-icon la la-home"></i>
          {{/link-to}}
        </li>
        <li class="m-nav__separator">
          -
        </li>
        <li class="m-nav__item">
          {{#link-to 'lang.referential' class="m-nav__link m-nav__link--icon"}}
            <span class="m-nav__link-text">
              {{t "index.referential.title"}}
            </span>
          {{/link-to}}
        </li>
        <li class="m-nav__separator">
          -
        </li>
        <li class="m-nav__item">
          {{#link-to 'lang.referential.make' model.vehicle.make.id class="m-nav__link m-nav__link--icon"}}
            <span class="m-nav__link-text">
              {{model.vehicle.make.name}}
            </span>
          {{/link-to}}
        </li>
        <li class="m-nav__separator">
          -
        </li>
        <li class="m-nav__item">
          {{#link-to 'lang.referential.model.vehicle' model.vehicle.submodel.vehicle.id class="m-nav__link m-nav__link--icon"}}
            <span class="m-nav__link-text">
              {{model.vehicle.submodel.vehicle.name}}
            </span>
          {{/link-to}}
        </li>
      </ul>
    </div>
  </div>
</div>
<div class="m-content">
  <div class="row">
    <div class="col-4">
      <div class="m-portlet">
        <div class="m-portlet__head">
          <div class="m-portlet__head-caption">
            <div class="m-portlet__head-title">
              <h3 class="m-portlet__head-text">Vehicle Identity</h3>
            </div>
          </div>
        </div>
        <div class="m-portlet__body">
          <div class="vehicule-identity">
            <div class="vehicule-identity__make-logo m--margin-right-15">
              {{logo-img url=model.vehicle.make.logoURL className='make-logo'}}
            </div>
            <div class="vehicule-infos">
              <h4 class="vehicule-infos__make m--marginless">{{model.vehicle.make.name}}</h4>
              <h5 class="vehicule-infos__model.vehicle m--marginless">{{model.vehicle.phase.shortNicename}}</h5>
              <p class="vehicule-infos__name m--marginless">{{model.vehicle.name}}</p>
              <p class="vehicule-infos__body-type m--marginless">{{model.vehicle.generation.bodyType}}</p>
              <p class="vehicule-infos__segment m--margin-bottom-10">{{model.vehicle.generation.segment}}</p>
            </div>
          </div>
          <p class="vehicule-infos__date text-right font-italic m--marginless">{{model.vehicle.generation.displayDate}}</p>
        </div>
      </div>
      {{#sticky-element top=110}}
        <div class="m-portlet">
          <div class="m-portlet__head">
            <div class="m-portlet__head-caption">
              <div class="m-portlet__head-title">
                <h3 class="m-portlet__head-text">Price and cote</h3>
              </div>
            </div>
          </div>
          <div class="m-portlet__body">
            <h4>Price evolution</h4>
            <div class="m--margin-30 text-center">Graph</div>
            <h4>Quote</h4>
            <div class="quote-price">
              <table class="quote-infos">
                <tr>
                  <th class="m--padding-right-10 text-right m--font-bold">Quotable</th>
                  <td>{{if mode.quotable "yes" "no"}}</td>
                </tr>
                <tr>
                  <th class="m--padding-right-10 text-right m--font-bold">First quote</th>
                  <td>{{moment-format model.vehicle.firstQuoteAt "DD/MM/YYYY"}}</td>
                </tr>
                <tr>
                  <th class="m--padding-right-10 text-right m--font-bold">Prevarable</th>
                  <td>{{if model.vehicle.prevarable "yes" "no"}}</td>
                </tr>
              </table>
              <div class="vehicle-price text-right m--bg-metal m--font-light m--padding-15">
                <p class="price-incl-taxes mb-0 m--font-bold">{{selectedPeriod.content.priceIncludingVat}}€ <span class="m--font-thin">incl. tax</span></p>
                <p class="price-excl-taxes mb-0 m--font-normal">{{selectedPeriod.content.priceExcludingVat}}€ <span class="m--font-thin">excl. taxes</span></p>
              </div>
            </div>
          </div>
        </div>
      {{/sticky-element}}
    </div>
    <div class="col-8">
      <div class="m-portlet">
        <div class="m-portlet__head">
          <div class="m-portlet__head-caption">
            <div class="m-portlet__head-title">
              <h3 class="m-portlet__head-text">Dimension and weight</h3>
            </div>
          </div>
        </div>
        <div class="m-portlet__body">
          <div class="row">
            <div class="col-6">
              {{#tech-data-container titleKey="techData.dimensions.title"}}
                {{tech-data-row labelKey="techData.dimensions.length" value=selectedPeriod.content.dimension.length unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.dimensions.width" value=selectedPeriod.content.dimension.width unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.dimensions.height" value=selectedPeriod.content.dimension.height unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.dimensions.heightIncludingRoofRails" value=selectedPeriod.content.dimension.heightIncludingRoofRails unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.dimensions.wheelBase" value=selectedPeriod.content.dimension.wheelBase unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.dimensions.fuelTank" value=selectedPeriod.content.dimension.fuelTankCapacity unitKey="techData.units.liter"}}
                {{tech-data-row labelKey="techData.dimensions.frontOverhang" value=selectedPeriod.content.dimension.frontOverhang unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.dimensions.rearOverhang" value=selectedPeriod.content.dimension.rearOverhang unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.dimensions.frontTrack" value=selectedPeriod.content.dimension.frontTrack unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.dimensions.rearTrack" value=selectedPeriod.content.dimension.rearTrack unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.dimensions.dragCoefficent" value=selectedPeriod.content.dimension.dragCoefficent unitKey="techData.units.cx"}}
              {{/tech-data-container}}
              {{#tech-data-container titleKey="techData.tyre.title"}}
                {{tech-data-row labelKey="techData.tyre.wheelType" value=selectedPeriod.content.tyre.wheelType}}
                {{tech-data-row labelKey="techData.tyre.tyreType" value=selectedPeriod.content.tyre.constructionType}}
                {{tech-data-row labelKey="techData.tyre.front" value=selectedPeriod.content.tyre.front}}
                {{tech-data-row labelKey="techData.tyre.rear" value=selectedPeriod.content.tyre.rear}}
                {{tech-data-row labelKey="techData.tyre.sparewheelType" value=selectedPeriod.content.tyre.sparewheelType}}
              {{/tech-data-container}}
            </div>
            <div class="col-6">
              {{#tech-data-container titleKey="techData.weight.title"}}
                {{tech-data-row labelKey="techData.weight.kerbweight" value=selectedPeriod.content.weight.kerbweight unitKey="techData.units.kg"}}
                {{tech-data-row labelKey="techData.weight.kerbweightIncludingDriver" value=selectedPeriod.content.weight.kerbweightIncludingDriver unitKey="techData.units.kg"}}
                {{tech-data-row labelKey="techData.weight.grossVehicleRating" value=selectedPeriod.content.weight.grossVehicleRating unitKey="techData.units.kg"}}
                {{tech-data-row labelKey="techData.weight.grossCombinedRating" value=selectedPeriod.content.weight.grossCombinedRating unitKey="techData.units.kg"}}
                {{tech-data-row labelKey="techData.weight.payload" value=selectedPeriod.content.weight.payload unitKey="techData.units.kg"}}
                {{tech-data-row labelKey="techData.weight.unbrakedTrailer" value=selectedPeriod.content.weight.unbrakedTrailer unitKey="techData.units.kg"}}
              {{/tech-data-container}}
              {{#tech-data-container titleKey="techData.habitability.title"}}
                {{tech-data-row labelKey="techData.habitability.numberOfPlaces" value=model.vehicle.numberOfPlaces}}
                {{tech-data-row labelKey="techData.habitability.numberOfDoors" value=model.vehicle.numberOfDoors}}
              {{/tech-data-container}}
              {{#tech-data-container titleKey="techData.boot.title"}}
                {{tech-data-row labelKey="techData.boot.capacity" value=selectedPeriod.content.boot.capacity unitKey="techData.units.liter"}}
                {{tech-data-row labelKey="techData.boot.minimumCapacity" value=selectedPeriod.content.boot.minimumCapacity unitKey="techData.units.liter"}}
                {{tech-data-row labelKey="techData.boot.maximumCapacity" value=selectedPeriod.content.boot.maximumCapacity unitKey="techData.units.liter"}}
                {{tech-data-row labelKey="techData.boot.maximumLength" value=selectedPeriod.content.boot.maximumlength unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.boot.usableLength" value=selectedPeriod.content.boot.usableLength unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.boot.usableWidth" value=selectedPeriod.content.boot.usableLength unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.boot.loadStillHeight" value=selectedPeriod.content.boot.loadStillHeight unitKey="techData.units.mm"}}
                {{tech-data-row labelKey="techData.boot.thirdRowCapacity" value=selectedPeriod.content.boot.thirdRowCapacity unitKey="techData.units.liter"}}
              {{/tech-data-container}}
            </div>
          </div>
        </div>
      </div>
      <div class="m-portlet">
        <div class="m-portlet__head">
          <div class="m-portlet__head-caption">
            <div class="m-portlet__head-title">
              <h3 class="m-portlet__head-text">Technical specifications</h3>
            </div>
          </div>
        </div>
        <div class="m-portlet__body">
          <div class="row">
            <div class="col-6">
              {{#tech-data-container titleKey="techData.energy.title"}}
                {{tech-data-row labelKey="techData.energy.name" value=model.vehicle.energy.name}}
                {{tech-data-row labelKey="techData.energy.code" value=model.vehicle.energy.code}}
                {{tech-data-row labelKey="techData.energy.masterId" value=model.vehicle.energy.masterId}}
                {{tech-data-row labelKey="techData.energy.masterName" value=model.vehicle.energy.masterName}}
                {{tech-data-row labelKey="techData.energy.masterCode" value=model.vehicle.energy.masterCode}}
                {{tech-data-row labelKey="techData.energy.legacyId" value=model.vehicle.energy.legacyId}}
              {{/tech-data-container}}
              {{#tech-data-container titleKey="techData.engine.title"}}
                {{tech-data-row labelKey="techData.engine.marketName" value=selectedPeriod.content.engine.marketName}}
                {{tech-data-row labelKey="techData.engine.powerMarketName" value=selectedPeriod.content.engine.powerMarketName}}
                {{tech-data-row labelKey="techData.engine.fullNicename" value=selectedPeriod.content.engine.fullNicename}}
                {{tech-data-row labelKey="techData.engine.acronym" value=selectedPeriod.content.engine.acronym}}
                {{tech-data-row labelKey="techData.engine.layout" value=selectedPeriod.content.engine.layout}}
                {{tech-data-row labelKey="techData.engine.configuration" value=selectedPeriod.content.engine.configuration}}
                {{tech-data-row labelKey="techData.engine.supply" value=selectedPeriod.content.engine.supply}}
                {{tech-data-row labelKey="techData.engine.injectionSystem" value=selectedPeriod.content.engine.injectionSystem}}
                {{tech-data-row labelKey="techData.engine.literCapacity" value=selectedPeriod.content.engine.literCapacity unitKey="techData.units.liter"}}
                {{tech-data-row labelKey="techData.engine.cubicCapacity" value=selectedPeriod.content.engine.cubicCapacity unitKey="techData.units.cc"}}
                {{tech-data-row labelKey="techData.engine.dinHorsepower" value=selectedPeriod.content.engine.dinHorsepower unitKey="techData.units.hp"}}
                {{tech-data-row labelKey="techData.engine.kilowatt" value=selectedPeriod.content.engine.kilowatt unitKey="techData.units.kw"}}
                {{tech-data-row labelKey="techData.engine.maxPowerRpm" value=selectedPeriod.content.engine.maxPowerRpm unitKey="techData.units.rpm"}}
                {{tech-data-row labelKey="techData.engine.torque" value=selectedPeriod.content.engine.torque unitKey="techData.units.nm"}}
                {{tech-data-row labelKey="techData.engine.maxTorqueRpm" value=selectedPeriod.content.engine.maxTorqueRpm unitKey="techData.units.rpm"}}
                {{tech-data-row labelKey="techData.engine.numberOfValves" value=selectedPeriod.content.engine.numberOfValves}}
                {{tech-data-row labelKey="techData.engine.boreAndStroke" value=selectedPeriod.content.engine.boreAndStroke}}
                {{tech-data-row labelKey="techData.engine.compressionRatio" value=selectedPeriod.content.engine.compressionRatio}}
                {{tech-data-row labelKey="techData.engine.emission" value=selectedPeriod.content.engine.emission}}
                {{tech-data-row labelKey="techData.engine.fiscalHorsepower" value=selectedPeriod.content.engine.fiscalHorsepower unitKey="techData.units.cv"}}
              {{/tech-data-container}}
            </div>
            <div class="col-6">
              {{#tech-data-container titleKey="techData.gearbox.title"}}
                {{tech-data-row labelKey="techData.gearbox.name" value=model.vehicle.gearbox.name}}
                {{tech-data-row labelKey="techData.gearbox.marketingName" value=model.vehicle.gearbox.marketingName}}
                {{tech-data-row labelKey="techData.gearbox.subtype" value=model.vehicle.gearbox.subtype}}
                {{tech-data-row labelKey="techData.gearbox.numberOfGears" value=model.vehicle.gearbox.numberOfGears}}
                {{tech-data-row labelKey="techData.gearbox.code" value=model.vehicle.gearbox.code}}
                {{tech-data-row labelKey="techData.gearbox.legacyId" value=model.vehicle.gearbox.legacyId}}
              {{/tech-data-container}}
              {{#tech-data-container titleKey="techData.transmission.title"}}
                {{tech-data-row labelKey="techData.transmission.marketingName" value=selectedPeriod.content.transmission.marketingName}}
                {{tech-data-row labelKey="techData.transmission.drivenWheels" value=selectedPeriod.content.transmission.drivenWheels}}
                {{tech-data-row labelKey="techData.transmission.numberOfGears" value=selectedPeriod.content.transmission.numberOfGears}}
              {{/tech-data-container}}
              {{#tech-data-container titleKey="techData.platform.title"}}
                {{tech-data-row labelKey="techData.platform.chassisType" value=selectedPeriod.content.platform.chassisType}}
                {{tech-data-row labelKey="techData.platform.chassisMaterial" value=selectedPeriod.content.platform.chassisMaterial}}
                {{tech-data-row labelKey="techData.platform.powerSteering" value=selectedPeriod.content.platform.powerSteering}}
                {{tech-data-row labelKey="techData.platform.powerSteeringType" value=selectedPeriod.content.platform.powerSteeringType}}
                {{tech-data-row labelKey="techData.platform.powerSteeringAssistance" value=selectedPeriod.content.platform.powerSteeringAssistance}}
                {{tech-data-row labelKey="techData.platform.turningCircleBetweenKerbs" value=selectedPeriod.content.platform.turningCircleBetweenKerbs unitKey="techData.units.m"}}
                {{tech-data-row labelKey="techData.platform.turningCircleWallToWall" value=selectedPeriod.content.platform.turningCircleWallToWall unitKey="techData.units.m"}}
                {{tech-data-row labelKey="techData.platform.steeringWheelLockToLockTurns" value=selectedPeriod.content.platform.steeringWheelLockToLockTurns unitKey="techData.units.degree"}}
                {{tech-data-row labelKey="techData.platform.frontSuspension" value=selectedPeriod.content.platform.frontSuspension}}
                {{tech-data-row labelKey="techData.platform.rearSuspensionType" value=selectedPeriod.content.platform.rearSuspensionType}}
              {{/tech-data-container}}
            </div>
          </div>
        </div>
      </div>
      <div class="m-portlet">
        <div class="m-portlet__head">
          <div class="m-portlet__head-caption">
            <div class="m-portlet__head-title">
              <h3 class="m-portlet__head-text">Perfomances and consumption</h3>
            </div>
          </div>
        </div>
        <div class="m-portlet__body">
          <div class="row">
            <div class="col-6">
              {{#tech-data-container titleKey="techData.performance.title"}}
                {{tech-data-row labelKey="techData.performance.maximumSpeed" value=selectedPeriod.content.performance.maximumSpeed unitKey="techData.units.kph"}}
                {{tech-data-row labelKey="techData.performance.da0100kph" value=selectedPeriod.content.performance.da0100kph unitKey="techData.units.sec"}}
                {{tech-data-row labelKey="techData.performance.da01000m" value=selectedPeriod.content.performance.da01000m unitKey="techData.units.sec"}}
              {{/tech-data-container}}
            </div>
            <div class="col-6">
              {{#tech-data-container titleKey="techData.consumption.title"}}
                {{tech-data-row labelKey="techData.consumption.urbanFuel" value=selectedPeriod.content.consumption.urbanFuel unitKey="techData.units.l100km"}}
                {{tech-data-row labelKey="techData.consumption.extraUrbanFuel" value=selectedPeriod.content.consumption.extraUrbanFuel unitKey="techData.units.l100km"}}
                {{tech-data-row labelKey="techData.consumption.combinedFuel" value=selectedPeriod.content.consumption.combinedFuel unitKey="techData.units.l100km"}}
                {{tech-data-row labelKey="techData.consumption.electricRange" value=selectedPeriod.content.consumption.electricRange unitKey="techData.units.km"}}
                {{tech-data-row labelKey="techData.consumption.co2EmissionLevel" value=selectedPeriod.content.consumption.co2EmissionLevel unitKey="techData.units.gpkm"}}
                {{tech-data-row labelKey="techData.consumption.carbonMonoxide" value=selectedPeriod.content.consumption.carbonMonoxide unitKey="techData.units.gpkm"}}
                {{tech-data-row labelKey="techData.consumption.nitrousOxyde" value=selectedPeriod.content.consumption.nitrousOxyde unitKey="techData.units.gpkm"}}
                {{tech-data-row labelKey="techData.consumption.particulateMatter" value=selectedPeriod.content.consumption.particulateMatter unitKey="techData.units.gpkm"}}
                {{tech-data-row labelKey="techData.consumption.totalHydrocarbons" value=selectedPeriod.content.consumption.totalHydrocarbons unitKey="techData.units.gpkm"}}
              {{/tech-data-container}}
            </div>
          </div>
        </div>
      </div>
      <div class="m-portlet">
        <div class="m-portlet__head">
          <div class="m-portlet__head-caption">
            <div class="m-portlet__head-title">
              <h3 class="m-portlet__head-text">Standard equipments</h3>
            </div>
          </div>
        </div>
        <div class="m-portlet__body">
          {{#if selectedPeriod.content}}
            {{#each model.featureCategories as |featureCategory|}}
              {{category-standard-equipments category=featureCategory equipments=selectedPeriod.content.standardEquipments}}
            {{/each}}
          {{/if}}
        </div>
      </div>
      {{#if selectedPeriod.content.optionalEquipments.length}}
        <div class="m-portlet">
          <div class="m-portlet__head">
            <div class="m-portlet__head-caption">
              <div class="m-portlet__head-title">
                <h3 class="m-portlet__head-text">Optional equipments</h3>
              </div>
            </div>
          </div>
          <div class="m-portlet__body">
            <table class="optional-equipement-table">
              <thead>
                <tr class="optional-equipement-table__header">
                  <th class="m--padding-top-15 m--padding-bottom-15 m--padding-left-10 m--padding-right-10">#</th>
                  <th class="m--padding-top-15 m--padding-bottom-15 m--padding-left-10 m--padding-right-10">Manufacturer ref.</th>
                  <th class="m--padding-top-15 m--padding-bottom-15 m--padding-left-10 m--padding-right-10">Name</th>
                  <th class="m--padding-top-15 m--padding-bottom-15 m--padding-left-10 m--padding-right-10">Category</th>
                  <th class="text-right m--padding-top-15 m--padding-bottom-15 m--padding-left-10 m--padding-right-10">Price excl. taxes</th>
                  <th class="text-right m--padding-top-15 m--padding-bottom-15 m--padding-left-10 m--padding-right-10">Price incl. taxes</th>
                </tr>
              </thead>
              <tbody>
                {{#each selectedPeriod.content.optionalEquipments as |equipment|}}
                <tr class="optional-equipement-table__row">
                  <td class="m--padding-10">{{equipment.id}}</td>
                  <td class="m--padding-10">{{equipment.manufacturerSpec}}</td>
                  <td class="m--padding-10">{{equipment.name}}</td>
                  <td class="m--padding-10">{{equipment.featureCategory.name}}</td>
                  <td class="text-right m--padding-10">{{equipment.priceExcludingVat}} €</td>
                  <td class="text-right m--padding-10">{{equipment.priceIncludingVat}} €</td>
                </tr>
                {{/each}}
              </tbody>
            </table>
          </div>
        </div>
      {{/if}}
    </div>
  </div>
</div>
