<h2>{{t "index.showroom.model.submodels"}}</h2>
<ul class="list-group showroom-flex-list">
  {{#each model.submodels as |modelRecord|}}
    <li class="list-group-item showroom-flex-list-item">
      <h3>{{modelRecord.name}}</h3>
      <p>{{modelRecord.displayDate}}</p>
      <p>{{modelRecord.id}}</p>
      {{#link-to "lang.showroom.make.model.submodel" modelRecord}}{{/link-to}}
    </li>
  {{/each}}
</ul>
